{
  "manifest_version": 3,
  "name": "Gemini MCP Connector",
  "version": "1.0",
  "permissions": ["nativeMessaging", "scripting"],
  "host_permissions": ["https://gemini.google.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/*"],
      "js": ["content.js"]
    }
  ]
}